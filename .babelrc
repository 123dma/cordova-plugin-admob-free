{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "> 1%",
          "last 4 versions",
          "Android > 2",
          "last 2 ChromeAndroid versions"
        ]
      }
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        ["module-resolver", {
          "alias": {
            "AdMob":  "./www/AdMob.js",
            "cordova/exec": "./tests/js/stub/cordova-exec.js"
          }
        }]
      ]
    }
  },
  "plugins": [
    "add-module-exports"
  ]
}
