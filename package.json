{
  "name": "cordova-plugin-admob-free",
  "version": "0.0.4",
  "description": "Cordova AdMob Plugin for Google AdMob",
  "scripts": {
    "clean": "rimraf www",
    "build": "babel src/js --out-dir www",
    "prepublish": "npm-run-all clean build"
  },
  "cordova": {
    "id": "cordova-plugin-admob-free",
    "platforms": [
      "android",
      "ios",
      "wp8",
      "windows"
    ]
  },
  "repository": "ratson/cordova-plugin-admob-free",
  "keywords": [
    "cordova",
    "admob",
    "ecosystem:cordova",
    "cordova-plugin-admob",
    "cordova-admob",
    "plugin",
    "google",
    "admobpro"
  ],
  "tonicExampleFilename": "test.js",
  "author": {
    "name": "Ratson"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "npm-run-all": "^2.1.1",
    "rimraf": "^2.5.2"
  },
  "files": [
    "*.js",
    "*.md",
    "*.xml",
    "LICENSE",
    "src",
    "www"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
