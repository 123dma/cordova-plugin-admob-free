{
  "name": "cordova-plugin-admob-free",
  "version": "0.1.5",
  "description": "Cordova AdMob Plugin for Google AdMob",
  "scripts": {
    "clean": "rimraf www",
    "build": "babel src/js --out-dir www",
    "prepublish": "npm-run-all clean build",
    "version": "sync-package-version --cordova-plugin"
  },
  "cordova": {
    "id": "cordova-plugin-admob-free",
    "platforms": [
      "android",
      "ios",
      "wp8"
    ]
  },
  "repository": "ratson/cordova-plugin-admob-free",
  "keywords": [
    "cordova",
    "admob",
    "ecosystem:cordova",
    "cordova-plugin-admob",
    "cordova-admob",
    "cordova-android",
    "cordova-ios",
    "cordova-windows",
    "cordova-wp8",
    "plugin",
    "google",
    "admobpro"
  ],
  "tonicExampleFilename": "test.js",
  "author": "Ratson",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "npm-run-all": "^3.1.0",
    "rimraf": "^2.5.4",
    "sync-package-version": "0.0.3"
  },
  "files": [
    "*.js",
    "*.md",
    "*.xml",
    "LICENSE",
    "src",
    "www"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
