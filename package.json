    "platforms": [
      "android",
      "ios"
    ]
  },
  "repository": "ratson/cordova-plugin-admob-free",
  "keywords": [
    "cordova",
    "admob",
    "ecosystem:cordova",
    "cordova-plugin-admob",
    "cordova-admob",
    "cordova-android",
    "cordova-ios",
    "plugin",
    "google",
    "admobpro"
  ],
  "tonicExampleFilename": "test.js",
  "author": "Ratson",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "0.0.9",
    "cordova-android": "^6.1.0",
    "cordova-test-cli": "0.0.1",
    "npm-run-all": "^3.1.2",
    "rimraf": "^2.5.4",
    "sync-package-version": "0.0.3"
  },
  "files": [
    "*.js",
    "*.md",
    "*.xml",
    "LICENSE",
    "src",
    "www"
  ]
